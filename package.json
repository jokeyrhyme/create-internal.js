{
  "name": "create-internal",
  "description": "simple storage and access of private values, powered by WeakMaps",
  "version": "1.0.1",
  "author": "Ron Waldon <jokeyrhyme@gmail.com> (https://github.com/jokeyrhyme)",
  "bugs": {
    "url": "https://github.com/jokeyrhyme/create-internal.js/issues"
  },
  "devDependencies": {
    "eslint": "^3.12.2",
    "eslint-config-standard": "^8.0.0-beta.1",
    "eslint-plugin-node": "^4.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.41.0",
    "jest": "^19.0.0"
  },
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jokeyrhyme/create-internal.js#readme",
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "testRegex": "/__tests__/[^\\/]*\\.js$"
  },
  "keywords": [
    "class",
    "internal",
    "namespace",
    "private",
    "weakmap"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jokeyrhyme/create-internal.js.git"
  },
  "scripts": {
    "eslint": "eslint --fix --cache .",
    "fixpack": "fixpack",
    "flow_check": "flow check",
    "jest": "jest",
    "posttest": "npm run eslint && npm run flow_check && npm run fixpack",
    "test": "npm run jest"
  }
}
